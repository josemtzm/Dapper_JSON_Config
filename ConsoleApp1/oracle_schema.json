            "Char",
            "Clob",
            "Date",
            "Decimal",
            "Double",
            "Long",
            "LongRaw",
            "Int16",
            "Int32",
            "Int64",
            "IntervalDS",
            "IntervalYM",
            "NClob",
            "NChar",
            "NVarchar2",
            "Raw",
            "RefCursor",
            "Single",
            "TimeStamp",
            "TimeStampLTZ",
            "TimeStampTZ",
            "Varchar2",
            "XmlType",
            "BinaryDouble",
            "BinaryFloat",
            "Boolean"
          ]
        },
        "Direccion": {
          "enum": [ "Input", "Output" ]
        },
        "AtributosEntd": {
          "type": "object",
          "items": { "$ref": "#/definitions/AtributosEntd" }
        },
